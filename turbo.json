{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"check:format": {},
		"check:lint": {},
		"check:types": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"fix:format": {},
		"fix:lint": {}
	}
}
