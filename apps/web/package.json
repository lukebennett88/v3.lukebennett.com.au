{
	"name": "@lukebennett/web",
	"private": true,
	"type": "module",
	"scripts": {
		"astro": "astro",
		"build": "astro build",
		"check": "pnpm exec prettier 'src/**/*.astro' --cache --check && pnpm biome check && pnpm run check:types",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
		"clean": "pnpm clean:outputs && rm -rf node_modules",
		"clean:outputs": "rm -rf .astro .turbo tsconfig.tsbuildinfo",
		"dev": "astro dev --host 127.0.0.1",
		"fix": "pnpm biome check --write",
		"fix:format": "pnpm exec prettier 'src/**/*.astro' --cache --write && pnpm biome format --write",
		"fix:lint": "pnpm biome lint --write",
		"lint": "pnpm biome lint --write",
		"preview": "astro preview",
		"start": "astro dev"
	},
	"dependencies": {
		"@astrojs/markdoc": "0.11.4",
		"@astrojs/react": "3.6.2",
		"@astrojs/rss": "4.0.7",
		"@astrojs/sitemap": "3.1.6",
		"@astrojs/tailwind": "5.1.1",
		"@astrojs/vercel": "7.8.1",
		"@fontsource-variable/source-code-pro": "5.1.0",
		"@fontsource-variable/source-sans-3": "5.1.0",
		"@fontsource-variable/source-serif-4": "5.1.0",
		"@keystatic/astro": "5.0.0",
		"@keystatic/core": "0.5.36",
		"@sindresorhus/slugify": "2.2.1",
		"astro": "4.15.8",
		"clsx": "2.1.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"shiki": "1.18.0",
		"tailwind-merge": "2.5.2",
		"tiny-invariant": "1.3.3"
	},
	"devDependencies": {
		"@lukebennett/tsconfig": "workspace:^",
		"@tailwindcss/typography": "0.5.15",
		"@types/react": "18.3.11",
		"@types/react-dom": "18.3.0",
		"tailwindcss": "3.4.12",
		"typescript": "5.6.2"
	}
}
