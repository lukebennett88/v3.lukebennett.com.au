{
	"name": "@lukebennett/web",
	"private": true,
	"type": "module",
	"scripts": {
		"astro": "astro",
		"build": "astro build",
		"check": "astro check && pnpm exec prettier 'src/**/*.astro' --cache --check && pnpm biome check && pnpm run check:types",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
		"clean": "pnpm clean:outputs && rm -rf node_modules",
		"clean:outputs": "rm -rf .astro .turbo tsconfig.tsbuildinfo",
		"dev": "astro dev --host 127.0.0.1",
		"fix": "pnpm biome check --write",
		"fix:format": "pnpm exec prettier 'src/**/*.astro' --cache --write && pnpm biome format --write",
		"fix:lint": "pnpm biome lint --write",
		"lint": "pnpm biome lint --write",
		"preview": "astro preview",
		"start": "astro dev"
	},
	"dependencies": {
		"@astrojs/markdoc": "^0.12.4",
		"@astrojs/react": "^4.1.2",
		"@astrojs/rss": "^4.0.10",
		"@astrojs/sitemap": "^3.2.1",
		"@astrojs/tailwind": "^5.1.4",
		"@astrojs/vercel": "^8.0.1",
		"@fontsource-variable/source-code-pro": "^5.1.1",
		"@fontsource-variable/source-sans-3": "^5.1.1",
		"@fontsource-variable/source-serif-4": "^5.1.1",
		"@keystatic/astro": "^5.0.3",
		"@keystatic/core": "^0.5.42",
		"@sindresorhus/slugify": "^2.2.1",
		"astro": "^5.1.1",
		"clsx": "^2.1.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"shiki": "^1.24.4",
		"tailwind-merge": "^2.6.0",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@lukebennett/tsconfig": "workspace:^",
		"@tailwindcss/typography": "^0.5.15",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"prettier": "^3.4.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"react-compiler-runtime": "19.0.0-beta-201e55d-20241215",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.2"
	}
}
